singleGraph
{
    //start   ( 0.005 0.015 0.0 );
    start   ( 0.005 0.0 0.0 );
    end     ( 0.005 0.0999 0.0);
    fields  ( Ax Bz );

    #includeEtc "caseDicts/postProcessing/graphs/sampleDict.cfg"

    setConfig
    {
        axis    y;
        nPoints 500;
    }

    // Must be last entry
    #includeEtc "caseDicts/postProcessing/graphs/graph.cfg"
}

